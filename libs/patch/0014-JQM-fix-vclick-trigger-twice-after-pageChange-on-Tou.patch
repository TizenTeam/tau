---
 .../js/jquery.mobile.vmouse.js                     |    4 ++++
 1 files changed, 4 insertions(+), 0 deletions(-)

diff --git a/libs/js/jquery-mobile-1.0.1pre/js/jquery.mobile.vmouse.js b/libs/js/jquery-mobile-1.0.1pre/js/jquery.mobile.vmouse.js
index 6fa6d8f..b98dda9 100644
--- a/libs/js/jquery-mobile-1.0.1pre/js/jquery.mobile.vmouse.js
+++ b/libs/js/jquery-mobile-1.0.1pre/js/jquery.mobile.vmouse.js
@@ -183,6 +183,10 @@ function triggerVirtualEvent( eventType, event, flags ) {
 function mouseEventCallback( event ) {
 	var touchID = $.data(event.target, touchTargetPropertyName);
 
+	if ( ( $.support.touch === true ) && ( touchID === undefined ) ) {
+		return;
+	}
+
 	if ( !blockMouseTriggers && ( !lastTouchID || lastTouchID !== touchID ) ){
 		var ve = triggerVirtualEvent( "v" + event.type, event );
 		if ( ve ) {
