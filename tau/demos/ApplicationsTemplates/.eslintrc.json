{
  "extends": "eslint:recommended",
  "rules": {
    "no-mixed-spaces-and-tabs": 2,
    "block-scoped-var": 2,
    "no-console": ["error", { "allow": ["warn", "error", "info"] }],
    "complexity": ["error", 19],
    "consistent-return": 2,
    "curly": 2,
    "dot-location": 2,
    "dot-notation": 2,
    "guard-for-in": 2,
    "no-alert": 2,
    "no-else-return": 2,
    "no-empty-function": 2,
    "no-extra-bind": 2,
    "no-multi-spaces": 2,
    "no-lone-blocks": 2,
    "no-unused-expressions": 2,
    "no-useless-call": 2,
    "radix": 2,
    "strict": 2,
    "init-declarations": 2,
    "no-use-before-define": 2,
    "array-bracket-spacing": 2,
    "block-spacing": 2,
    "brace-style": 2,
    "camelcase": 2,
    "newline-after-var": 2,
    "indent": ["error", "tab"],
    "one-var": 2,
    "one-var-declaration-per-line": 2,
    "require-jsdoc": ["error", {
      "require": {
        "FunctionDeclaration": false,
        "MethodDefinition": true,
        "ClassDeclaration": true
      }
    }]
  },
  "globals": {
    "console": true,
    "document": true,
    "tizen": true,
    "window": true,
    "define": true,
    "requirejs": true
  }
}
